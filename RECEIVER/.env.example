# === Configuration du Collecteur SNMP Unifié ===

# API REST
SNMP_API_URL=https://api.exemple.fr
SNMP_API_KEY=VOTRE_CLE_API_ICI

# Capture réseau (Windows: "Ethernet 2", Linux/Docker: eth0, ens18...)
SNMP_INTERFACE=eth0
SNMP_WORKERS=3
SNMP_PORT=162

# SNMPv3 (polling actif + déchiffrement)
SNMP_SWITCH_IP=10.0.0.1
SNMP_V3_USERNAME=UTILISATEUR_V3
SNMP_V3_AUTH_PASSWORD=MOT_DE_PASSE_AUTH
SNMP_V3_PRIV_PASSWORD=MOT_DE_PASSE_PRIV
SNMP_V3_ENGINE_ID=ENGINE_ID_HEX
SNMP_V3_PRIV_PROTOCOL=DES
SNMP_POLL_INTERVAL=60

# Détection avancée (spam, faux credentials, reconnaissance)
SNMP_WHITELIST_IPS=10.0.0.1,10.0.0.2
SNMP_KNOWN_COMMUNITIES=public,private
SNMP_KNOWN_V3_USERS=UTILISATEUR_V3
SNMP_FLOOD_THRESHOLD=50
SNMP_FLOOD_CRITICAL=200
SNMP_FLOOD_WINDOW=60
SNMP_BRUTEFORCE_THRESHOLD=5
SNMP_BRUTEFORCE_WINDOW=120
